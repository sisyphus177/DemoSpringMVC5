<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="demo.spring.model.entity.Account" table="account" schema="transactiontest">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="money">
            <column name="money" sql-type="double" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>